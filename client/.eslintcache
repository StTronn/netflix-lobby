nst results = await getSearchResults(title);\n      if (searching && title) {\n        setSearchList(results);\n        setMovieObj(results[0]);\n      }\n    };\n    fetchData();\n  }, [searching, title]);\n\n  useEffect(() => {\n    if (obj) {\n      if (obj.movieObj) setMovieObj(obj.movieObj);\n      if (obj.dateTime) setDateTime(new moment(obj.dateTime));\n      if (obj.movieObj && (obj.movieObj.title || obj.movieObj.original_name))\n        setTitle(obj.movieObj.title || obj.movieObj.original_name);\n    }\n  }, []);\n\n  const handleBlur = () => {\n    setSearching(false);\n    if (movieObj && (movieObj.title || movieObj.original_name) && title)\n      setTitle(movieObj.title || movieObj.original_name);\n  };\n\n  const setSearchingTrue = () => {\n    setSearching(true);\n  };\n\n  const HandleTitleChange = (e) => {\n    const value = e.target.value;\n    if (!value || value === \"\") {\n      setMovieObj({});\n    }\n    setTitle(value);\n  };\n\n  const imageUrl =\n    movieObj && movieObj.backdrop_path\n      ? baseUrl + movieObj.backdrop_path || movieObj.poster_path\n      : backgroundImage;\n\n  return (\n    <Header imageUrl={imageUrl}>\n      <BannerContents className=\"text-white md:px-16 sm:px-4\">\n        <div onFocus={setSearchingTrue} onBlur={handleBlur}>\n          <TitleInput\n            placeholder=\"Search for a Movie\"\n            value={title}\n            onChange={(e) => {\n              HandleTitleChange(e);\n            }}\n          />\n\n          {searching && <SearchList list={searchList} />}\n        </div>\n\n        {!searching && (\n          <div>\n            <Description>\n              {movieObj ? movieObj.overview || prompt : prompt}\n            </Description>\n            <InfoCointaiers>\n              <FaUserFriends className=\"inline mr-2\" />\n              {obj ? obj.users.length : 0} Members\n              <br />\n              <FaClock className=\"inline mr-2 text-white\" />\n              <DateComponent dateTime={dateTime} setDateTime={setDateTime} />\n            </InfoCointaiers>\n          </div>\n        )}\n      </BannerContents>\n    </Header>\n  );\n};\n\nconst Header = ({ children, imageUrl }) => (\n  <header\n    className=\"object-contain bg-white white\"\n    style={{\n      height: \"70vh\",\n      color: \"black\",\n      backgroundSize: \"cover\",\n      backgroundImage: `url(\n        ${imageUrl}\n        )`,\n      backgroundPosition: \"center center\",\n    }}\n  >\n    {children}\n  </header>\n);\n\nconst TitleInput = (props) => (\n  <input\n    className=\"text-4xl bg-transparent tracking-tight font-extrabold text-white sm:text-5xl border-none focus:outline-none md:text-6xl\"\n    {...props}\n  />\n);\n\nconst Description = ({ children }) => (\n  <p\n    style={{ minHeight: \"150px\" }}\n    className=\"text-base text-gray-300 sm:mt-5  sm:max-w-xl sm:mx-auto md:mt-5 lg:mx-0\"\n  >\n    {children}\n  </p>\n);\n\nconst InfoCointaiers = ({ children }) => (\n  <p className=\" text-white text-sm text-semibold sm:mt-5  sm:max-w-xl sm:mx-auto md:mt-5 lg:mx-0\">\n    {children}\n  </p>\n);\n\nconst prompt =\n  \"Select a movie to watch wait set the time and mood. Invite friends and stream together\";\nconst baseUrl = \"https://image.tmdb.org/t/p/original\";\nexport default Banner;\n","/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/components/Date/index.js",[],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/components/SearchList.js",[],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/components/Lobby/index.js",["108"],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/index.js",[],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/context/user.js",[],["109","110"],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/global-styles.js",[],"/home/rishav/Desktop/All_Files/#projex/netflix-alpha/client/src/reportWebVitals.js",[],"/home/rishav/Desktop/All_Files/#projex/netflix-alph